{
  "name": "books",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"node server/index.js\" \"next dev\"",
    "build": "concurrently --success first --kill-others --no-color --raw \"next build\" \"node server/index.js\"",
    "start": "concurrently --success first --kill-others --no-color --raw \"node server/index.js\" \"next start\"",
    "lint": "yarn run prettier; yarn run eslint; yarn run stylelint",
    "lint:fix": "yarn run prettier:fix; yarn run eslint:fix; yarn run stylelint:fix",
    "eslint": "eslint '**/*.{ts,tsx,json}'",
    "eslint:fix": "eslint --fix '**/*.{ts,tsx,json}'",
    "stylelint": "stylelint '**/*.{sass,scss,css,less}'",
    "stylelint:fix": "stylelint --fix '**/*.{sass,scss,css,less}'",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write ."
  },
  "dependencies": {
    "axios": "^0.21.1",
    "classnames": "^2.2.6",
    "formik": "^2.2.9",
    "next": "10.0.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "uniqid": "^5.3.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.11",
    "@types/json-server": "^0.14.3",
    "@types/node": "^14.14.7",
    "@types/react": "^16.9.56",
    "@types/uniqid": "^5.3.0",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "concurrently": "^6.2.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "json-server": "^0.16.3",
    "prettier": "2.2.1",
    "sass": "^1.29.0",
    "stylelint": "^13.10.0",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-prettier": "^8.0.2",
    "typescript": "^4.0.5"
  }
}
